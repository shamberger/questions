{
  "name": "react-webpack-node",
  "version": "2.0.1",
  "description": "Your One-Stop solution for a full-stack app with ES6/ES2015 React.js featuring universal Redux, React Router, React Router Redux Hot reloading, CSS modules, Express 4.x, and multiple ORMs.",
  "repository": "https://github.com/choonkending/react-webpack-node",
  "main": "index.js",
  "scripts": {
    "sequelize": "node_modules/.bin/sequelize",
    "lint": "eslint --ext .js,.jsx app server",
    "lint:fix": "eslint --ext .js,.jsx app server --fix",
    "clean": "rimraf public && rimraf compiled",
    "start": "cross-env NODE_ENV=production node compiled/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build:dev": "cross-env NODE_ENV=development webpack ---debug --colors --display-error-details --config ./webpack/webpack.config.dev-server.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --colors --display-error-details --config ./webpack/webpack.config.prod.js",
    "test": "cross-env NODE_ENV=test karma start",
    "test:watch": "cross-env NODE_ENV=test npm test -- --watch --no-single-run",
    "postinstall": "npm run build",
    "keep-green": "git merge master && rm -rf node_modules && npm install && npm start"
  },
  "author": "Artem Shamberger",
  "license": "MIT",
  "devDependencies": {
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "expect": "^1.20.2",
    "jsdom": "^9.8.3",
    "karma": "^1.3.0",
    "karma-jsdom-launcher": "^5.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "null-loader": "^0.1.1",
    "react-addons-test-utils": "^15.4.1",
    "react-transform-catch-errors": "^1.0.2",
    "redux-mock-store": "1.2.1",
    "sinon": "^1.17.6",
    "stylelint": "^7.6.0",
    "stylelint-webpack-plugin": "^0.4.0"
  },
  "engines": {
    "node": "5.x"
  },
  "dependencies": {
    "autoprefixer": "^6.5.3",
    "axios": "^0.15.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.11",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "connect-mongo": "^1.3.2",
    "connect-pg-simple": "^3.1.2",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "express-flash": "0.0.2",
    "express-session": "^1.14.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "helmet": "^3.1.0",
    "inline-environment-variables-webpack-plugin": "1.1.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.2",
    "method-override": "^2.3.7",
    "mongoose": "^4.7.1",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.0.0",
    "postcss-loader": "^1.2.0",
    "postcss-reporter": "^2.0.0",
    "postcss-simple-vars": "^3.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-helmet": "^3.2.3",
    "react-intl": "^2.1.5",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-transform-hmr": "^1.0.4",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
    "sequelize": "^3.27.0",
    "sequelize-cli": "^2.4.0",
    "spark-md5": "^3.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  }
}
